					<finalName>something</finalName>
                    <archive>
                        <manifest>
                            <mainClass>${pkgPrefix}.${projectKey}.cli.something.App</mainClass>
                        </manifest>
                    </archive>
                </configuration>
            </plugin>
        </plugins>
    </build>


